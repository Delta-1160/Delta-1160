#include<iostream>
#include<vector>
using namespace std;
//My code

void selectionsort(vector <int> arr , int n ){
	

	for(int i = 0; i<n-1;i++){
 int min = i;
	for (int j = i+1; j<n ; j++){
		if (arr[j] < arr[min]){
			
			min = j;
		
					
		}
						
		swap(arr[min] , arr [j])	;
	}		

cout<<arr[i];	
			}

			
}




int main(){
	
vector <int> arr={1,2,3,4,5};
int n = sizeof(arr) / sizeof(arr[0]);

	selectionsort( arr , n);	
	

}
	

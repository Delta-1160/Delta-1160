
#include <iostream>
using namespace std;
void PrintArray (int arr[],int n){
    for (int i=0; i<n ; i++){
        cout<<arr[i]<<" ";
        
    }cout<<endl;
}

void SwapAlternate (int arr[],int size)  {
     
 for (int i = 0 ; i<6; i+=2)  
 {
     if (i+1<6){
         swap (arr[i],arr[i+1]);
     
     }
 }
}

int main(){
    int even [8]={5,2,9,4,7,6,1,0};
    int odd [5] = {11,33,9,76,43};
    SwapAlternate(even ,8);
    PrintArray(even, 8);
    cout<<endl;
    
    SwapAlternate(odd,5);
    PrintArray(odd,5);
    return 0;
}

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

void PrintArray (int arr[],int n){
    for (int i =0 ; i<n;i++){
        cout<<arr[i]<<" ";
        
    }cout<<endl;
}



void SwapAlternate(int arr[],int size ){
    
    for (int i = 0 ; i<size;i+=2)
{
    if (i+1 < size){
        
        swap(arr[i], arr[i+1]);
        
    }
}    
}






int main() {
int  odd[6]={1,3,4,5,7,8};
 
  SwapAlternate(odd,6);
 PrintArray(odd , 6);
 
  
 
 
 
 
 return 0;
    
}
